<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DataOverviewActivity">

    <fragment
        android:id="@+id/dataOverview_fragment"
        class="com.github.HumanLearning2021.HumanLearningApp.DatasetOverViewFragment"
        android:layout_width="300dp"
        android:layout_height="500dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="112dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_dataset_over_view_list" />

    <Button
        android:id="@+id/dataOverviewButton"
        android:layout_width="153dp"
        android:layout_height="36dp"
        android:layout_marginStart="128dp"
        android:layout_marginBottom="52dp"
        android:onClick="create"
        android:text="@string/DataOverview_CreateButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="141dp"
        android:layout_height="47dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="44dp"
        android:text="@string/DataOverview_Ttile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>