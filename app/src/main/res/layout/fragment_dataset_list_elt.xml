<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/DatasetList_elt_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center|center_horizontal|center_vertical"
        android:text="@string/DatasetList_default_ds_name"
        android:textAppearance="?attr/textAppearanceListItem"
        app:layout_constraintBottom_toTopOf="@+id/DatasetList_elt_im1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/DatasetList_elt_im0"
        android:layout_width="@dimen/DatasetList_im_size"
        android:layout_height="@dimen/DatasetList_im_size"
        android:contentDescription="@string/DatasetList_im_content_description"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/DatasetList_elt_im1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DatasetList_elt_text"
        app:srcCompat="@color/red"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/DatasetList_elt_im1"
        android:layout_width="@dimen/DatasetList_im_size"
        android:layout_height="@dimen/DatasetList_im_size"
        android:contentDescription="@string/DatasetList_im_content_description"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/DatasetList_elt_im2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/DatasetList_elt_im0"
        app:layout_constraintTop_toBottomOf="@+id/DatasetList_elt_text"
        app:srcCompat="@color/green"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/DatasetList_elt_im2"
        android:layout_width="@dimen/DatasetList_im_size"
        android:layout_height="@dimen/DatasetList_im_size"
        android:contentDescription="@string/DatasetList_im_content_description"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/DatasetList_elt_im1"
        app:layout_constraintTop_toBottomOf="@+id/DatasetList_elt_text"
        app:srcCompat="@color/blue"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>